<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mobileauthentication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="MobileAuthentication"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileAuthentication"
        tools:targetApi="31">
        <activity
            android:name=".PhoneLogInActivity2"
            android:exported="false" />
        <activity
            android:name=".ResetPasswordActivity2"
            android:exported="false" />
        <activity
            android:name=".PhoneActivity2"
            android:exported="false" />
        <activity
            android:name=".HomeActivity2"
            android:exported="false" />
        <activity
            android:name=".GitActivity2"
            android:exported="false" />
        <activity
            android:name=".facebookActivity1"
            android:exported="false" />
        <activity
            android:name=".FaceBookActivity2"
            android:exported="false" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.facebook.sdk.ClientToken"
            android:value="@string/facebook_client_token" />

        <activity
            android:name=".GoogleActivity"
            android:exported="false" />
        <activity
            android:name=".HomeActivity3"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity2"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>